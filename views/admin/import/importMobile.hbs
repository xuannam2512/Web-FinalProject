<div class="panel panel-default">
    <div class="panel-heading">
        <div class="row">
            <div class="col-sm-10">
                <h4 class="text-center">Danh Sách Các Lần Nhập Hàng</h4>
            </div>
            <div class="col-sm-2">
                <a href="/admin/importMobile/newImport">
                    <button type="button" class="btn btn-secondary" style="float: right;">
                        Thêm
                    </button>
                </a>
            </div>
        </div>
    </div>
    <!-- /.panel-heading -->
    <div class="panel-body">
        <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
            <thead>
                <tr>
                    <th>Số loại sản phẩm</th>
                    <th>Ngày nhập</th>
                    <th>Tổng tiền</th>
                    <th>Chi tiết đơn nhập</th>
                </tr>
            </thead>
            <tbody>
                {{#each tables}}
                <tr>
                    <td>{{this.mobileImported.length}}</td>
                    <td>{{this.date}}</td>
                    <td>{{this.totalPrice}}</td>
                    <td>
                        <a href={{this.url}}>Xem chi tiết</a>
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
    <!-- /.panel-body -->
</div>