<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="text-center">Chi Tiết Đơn Hàng</h3>
    </div>
    <!-- /.panel-heading -->
    <div class="panel-body">
        <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
            <thead>
                <tr>
                    <th>Tên model</th>
                    <th>Tên nhà cung cấp</th>
                    <th>Số lượng nhập</th>
                    <th>Giá nhập</th>
                    <th>Chi tiết sản phẩm</th>
                </tr>
            </thead>
            <tbody>
                {{#each tables}}
                <tr>
                    <td>{{this.mobile.mobileName}}</td>
                    <td>{{this.mobile.provider.name}}</td>
                    <td>{{this.amount}}</td>
                    <td>{{this.price}}</td>
                    <td>
                        <a href="/admin/mobile/{{this.mobile._id}}">
                            Xem chi tiết
                        </a>
                    </td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
    <!-- /.panel-body -->
</div>